{
  "name": "@qikify/polaris-vue",
  "version": "0.5.3",
  "polaris_version": "9.2.2",
  "description": "Shopify Polaris UI library for Vue 3",
  "author": {
    "name": "Qikify"
  },
  "keywords": [
    "polaris",
    "shopify polaris",
    "polaris-vue",
    "vue3",
    "vue framework",
    "ui"
  ],
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "types": "./dist/types/src/polaris-vue.d.ts",
  "main": "./dist/polaris-vue.umd.js",
  "module": "./dist/polaris-vue.es.js",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build && rimraf dist/index.html dist/favicon.ico dist/images",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "rimraf ./docs && build-storybook --docs -o docs"
  },
  "dependencies": {
    "@shopify/polaris-icons": "^4.17.0",
    "lodash": "^4.17.21",
    "vite-svg-loader": "^3.1.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@rushstack/eslint-patch": "^1.1.0",
    "@shopify/typescript-configs": "^5.1.0",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-docs": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/addons": "^6.4.20",
    "@storybook/builder-vite": "^0.1.23",
    "@storybook/theming": "^6.4.20",
    "@storybook/vue3": "^6.4.20",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.3",
    "change-case": "^4.1.2",
    "eslint": "^8.10.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-vue": "^8.5.0",
    "hash-generator": "^0.1.0",
    "path": "^0.12.7",
    "polaris-react": "https://github.com/Shopify/polaris-react.git",
    "postcss": "^7",
    "postcss-modules": "^3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "storybook-dark-mode": "^1.0.9",
    "ts-dedent": "^2.2.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.6",
    "vite-plugin-checker": "^0.4.2",
    "vite-plugin-dts": "^1.0.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-replace": "^0.1.1",
    "vue-tsc": "^0.32.1"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qikify/polaris-vue.git"
  },
  "bugs": {
    "url": "https://github.com/qikify/polaris-vue/issues"
  },
  "homepage": "https://github.com/qikify/polaris-vue#readme",
